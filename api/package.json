    "bcrypt": "^5.1.1",
    "brazilian-values": "^0.13.0",
    "bull": "^4.12.7",
    "cache-manager": "^5.5.2",
    "cache-manager-redis-yet": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "moment": "^2.30.1",
    "nanoid": "^5.0.7",
    "nestjs-cls": "^4.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
